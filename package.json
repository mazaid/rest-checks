{
  "name": "mazaid-rest-checks",
  "version": "0.2.1",
  "description": "checks REST API",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf ./esdoc ./coverage",
    "docs:open": "npm run docs && open ./esdoc/index.html",
    "docs": "rm -rf ./esdoc && ./node_modules/.bin/esdoc ./ -c ./.esdoc.json",
    "tests": "./node_modules/.bin/mocha ./test/",
    "test": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha ./test/",
    "lint": "./node_modules/.bin/eslint -c .eslintrc.js api init models rest",
    "lint:fix": "./node_modules/.bin/eslint -c .eslintrc.js --fix  api init models rest"
  },
  "engines": {
    "node": ">=4"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mazaid/rest-checks.git"
  },
  "keywords": [
    "mazaid"
  ],
  "author": "alekzonder <alekzonder@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mazaid/rest-checks/issues"
  },
  "homepage": "https://github.com/mazaid/rest-checks#readme",
  "dependencies": {
    "joi": "^9.2.0",
    "lodash": "^4.16.4",
    "maf": "^0.4.6",
    "mazaid-error": "^0.3.1",
    "mazaid-rest-api-clients": "^0.1.6",
    "md5": "^2.2.1",
    "moment": "^2.15.1",
    "mongodb": "^2.2.10",
    "nprof": "^0.5.0",
    "repl-extra": "^0.1.3",
    "uuid": "^2.0.3"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "chai-json-schema": "^1.4.0",
    "codeclimate-test-reporter": "^0.3.3",
    "esdoc": "^0.4.8",
    "esdoc-node": "^1.0.0",
    "eslint": "^3.11.1",
    "ink-docstrap": "^1.2.1",
    "istanbul": "^0.4.4",
    "mocha": "^2.4.5"
  }
}
